{
  "name": "codeclimbers",
  "productName": "Code Climbers",
  "version": "0.1.9",
  "description": "The Code Climbers CLI",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  ],
  "jest": {
    "projects": [
      "packages/server/jest.config.js"
    ]
  },
  "scripts": {
    "build:app": "cd packages/app && npm run build",
    "build:server": "cd packages/server && npm run build",
    "build:command": "cd packages/commands && tsc -b",
    "build": "npx shx rm -rf dist && npm ci && npm run build:app && npm run build:server && npm run build:command",
    "dev:server": "cd packages/server && npm run start",
    "dev:app": "cd packages/app && npm run start",
    "check-deps": "node scripts/checkDependencies.js",
    "publish:npm": "npm run check-deps && npm run build && npm publish",
    "package": "npm run build && copyfiles -u 1 \\\"packages/**/*\\\" tmp/codeclimbers/packages && oclif pack tarballs",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --ext .ts,.tsx --fix .",
    "test": "jest",
    "test:watch": "cross-env JEST_WATCH=true jest --watch",
    "db:migrate": "knex migrate:latest",
    "db:migrate:make": "knex migrate:make"
  },
  "workspaces": [
    "packages/server",
    "packages/app",
    "packages/commands",
    "packages/config"
  ],
  "dependencies": {
    "@codeclimbers/config": "file:packages/config",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.16.4",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.16.4",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.3.10",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^4.0.2",
    "@oclif/core": "^4.0.12",
    "@oclif/plugin-warn-if-update-available": "^3.1.11",
    "@sentry/nestjs": "^8.24.0",
    "@sentry/profiling-node": "^8.24.0",
    "@tanstack/react-query": "^5.48.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cli-app": "file:packages/app",
    "cli-server": "file:packages/server",
    "dayjs": "^1.11.12",
    "find-process": "^1.4.7",
    "nestjs-knex": "^2.0.0",
    "picocolors": "^1.0.1",
    "react": "^18.3.1",
    "react-router-dom": "^6.24.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "server": "file:dist/server",
    "sqlite3": "^5.1.7",
    "ws": "^8.18.0"
  },
  "optionalDependencies": {
    "node-linux": "^0.1.12",
    "node-mac": "^1.0.1",
    "node-windows": "^1.0.0-beta.8"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.0.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.0.0",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.13.0",
    "vite": "^5.3.1"
  },
  "keywords": [],
  "author": {
    "name": "Paul Hovley",
    "email": "rphovley@gmail.com"
  },
  "files": [
    "/bin",
    "/dist"
  ],
  "bin": {
    "codeclimbers": "./bin/run.js",
    "codeclimbers-dev": "./bin/run-dev.js"
  },
  "oclif": {
    "bin": "codeclimbers",
    "commands": "./dist/commands",
    "dirname": "codeclimbers",
    "topicSeparator": ":",
    "plugins": [
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 1,
      "frequency": 10,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>.",
      "registry": "https://www.npmjs.com/package/@codeclimbers/cli"
    }
  }
}
